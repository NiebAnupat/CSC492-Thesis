generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model developer {
  developer_id Int     @id @default(autoincrement())
  email        String  @unique
  password     String
  role         roles   @default(developer)
  is_active    Boolean @default(true)
}

