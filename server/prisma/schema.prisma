  appointment_date       DateTime?               @db.Date
  appointment_time       DateTime?               @db.Time(6)
  is_nv_refer            Boolean?                @default(false)
  nv_operation_id        Int?
  quantity               Int?                    @default(1)
  is_notify              Boolean?                @default(false)
  status                 appointment_status?     @default(pending)
  create_at              DateTime?               @default(now()) @db.Timestamp(6)
  update_at              DateTime?               @default(now()) @db.Timestamp(6)
  deleted_at             DateTime?               @db.Timestamp(6)
  edit_by                String?                 @db.VarChar
  branch_id              Int
  branch                 branch                  @relation(fields: [branch_id], references: [branch_id], onDelete: NoAction, onUpdate: Cascade)
  dentist                dentist?                @relation(fields: [dentist_id], references: [dentist_id], onDelete: NoAction, onUpdate: Cascade)
  employee               employee?               @relation(fields: [edit_by], references: [employee_uid], onDelete: NoAction, onUpdate: Cascade)
  patient                patient?                @relation(fields: [hn], references: [hn], onDelete: NoAction, onUpdate: Cascade)
  treatment_nv_operation treatment_nv_operation? @relation(fields: [nv_operation_id], references: [nv_operation_id], onDelete: NoAction, onUpdate: Cascade)
}

